# Definition for singly-linked list.
# class ListNode:
#     def __init__(self, x):
#         self.val = x
#         self.next = None

class Solution:
    def deleteNode(self, node):
        while node.next:
            node.val = node.next.val
            if node.next.next:
                node = node.next
            else:
				# tail
                node.next = None
        """
        :type node: ListNode
        :rtype: void Do not return anything, modify node in-place instead.
        """
